{
    "name": "setono/google-analytics-events",
    "description": "A library that eases the generation of Google Analytics events",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "infection/infection": "^0.26",
        "nyholm/psr7": "^1.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18",
        "setono/code-quality-pack": "^2.4"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\GoogleAnalyticsEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\GoogleAnalyticsEvents\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        },
        "sort-packages": true
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
